{
    "version": 2,
    "builds": [
      {
        "src": "src/app/**/*",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/about", "dest": "/src/app/about/page" },
      { "src": "/contact", "dest": "/src/app/contact/page" },
      { "src": "/projects", "dest": "/src/app/projects/page" },
      { "src": "/", "dest": "/src/app/page" }
    ]
  }
  